{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "indent": "warn",
        "quotes": "warn",
        "semi": "warn",
        "no-unused-vars": "warn"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
